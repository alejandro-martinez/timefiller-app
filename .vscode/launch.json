  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach on iOS",
      "type": "nativescript",
      "request": "attach",
      "platform": "ios",
      "appRoot": "${workspaceRoot}/dist",
      "sourceMaps": true,
      "watch": false
    },
    {
      "name": "Attach on Android",
      "type": "nativescript",
      "request": "attach",
      "platform": "android",
      "appRoot": "${workspaceRoot}/dist",
      "sourceMaps": true,
      "watch": false
    }
  ]
}
